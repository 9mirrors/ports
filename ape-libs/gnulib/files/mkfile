CC=pcc
LD=pcc
CFLAGS=-c - -I. -Iapelibs -D_POSIX_SOURCE -D_SUSV2_SOURCE -DHAVE_CONFIG_H

install:V:
	mkdir -p /sys/include/ape/gnulib
	cp apelibs/*.h /sys/include/ape/gnulib/

nuke:V:
	mk clean
	rm -f $LIB
	rm -rf /sys/include/ape/gnulib
