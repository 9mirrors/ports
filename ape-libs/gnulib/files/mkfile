LD=pcc
CFLAGS=-c - -I. -I./apelibs -D_POSIX_SOURCE -D_SUSV2_SOURCE -DHAVE_CONFIG_H

install:V:
	mkdir -p /sys/include/ape/gnulib
	cp apelibs/*.h /sys/include/ape/gnulib/

